<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="mainImage" fitHeight="592.0" fitWidth="730.0" layoutX="21.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true" />
      <TextArea layoutX="769.0" layoutY="20.0" prefHeight="590.0" prefWidth="236.0" />
      <Button layoutX="884.0" layoutY="657.0" mnemonicParsing="false" onAction="#sendHome" prefHeight="79.0" prefWidth="124.0" text="HOME" />
      <HBox layoutX="34.0" layoutY="630.0" prefHeight="61.0" prefWidth="731.0">
         <children>
            <RadioButton mnemonicParsing="false" onAction="#showAcidNucleic" prefHeight="61.0" prefWidth="142.0" text="Acid Nucleic" />
            <RadioButton mnemonicParsing="false" onAction="#showCapsid" prefHeight="61.0" prefWidth="129.0" text="Capsid" />
            <RadioButton fx:id="envelopeButton" mnemonicParsing="false" onAction="#showEnvelope" prefHeight="61.0" prefWidth="132.0" text="Envelope" />
            <MenuButton fx:id="specialAttributes" mnemonicParsing="false" prefHeight="61.0" prefWidth="138.0" text="Special attributes">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
                <MenuItem mnemonicParsing="false" text="Action 2" />
              </items>
            </MenuButton>
         </children>
      </HBox>
      <Button layoutX="771.0" layoutY="657.0" mnemonicParsing="false" onAction="#playVideo" prefHeight="80.0" prefWidth="124.0" text="ENTRY VIDEO" />
   </children>
</AnchorPane>
